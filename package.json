{
  "name": "Framework Cypress Valtech",
  "version": "1.0.0",
  "description": "Framework development in Cypress-JS for web/mobile test automation",
  "main": "index.js",
  "scripts": {
    "lint": "npx standard",
    "lint:fix": "npx standard --fix",
    "test": "cypress run||true",
    "cy:open": "cypress open",
    "cleanup": "rm -fr cypress/results/*.xml",
    "merge-reports": "jrm combined.xml \"cypress/results/test-results-*.xml\""
  },
  "standard": {
    "globals": [
      "beforeEach",
      "context",
      "cy",
      "Cypress",
      "describe",
      "it"
    ]
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git clone + https://github.com/victorgeorges/Framework-Cypress-Valtech.git"
  },
  "author": "",
  "license": "Valtech Group",
  "bugs": {
    "url": "https://github.com/victorgeorges/Framework-Cypress-Valtech/issues"
  },
  "homepage": "https://github.com/victorgeorges/Framework-Cypress-Valtech#readme",
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.1",
    "cypress": "^9.5.1",
    "cypress-localstorage-commands": "^1.4.1",
    "faker": "^5.5.3",
    "junit-report-merger": "^3.0.5"
  }
}
